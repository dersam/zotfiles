"3.10.1 (created: 2015/08/25 00:41:59)

set complete=stl
set gui=none,tabs
colorscheme darkness

" iTerm-like mapping for moving tabs.
nnoremap <S-M-Left> :tabm -1<CR>
nnoremap <S-M-Right> :tabm +1<CR>
" Map regular Firefox search to Vim search, so I stop using Firefox.
nnoremap <M-f> /
" Map vimperator 'd' key for closing tabs to standard C+w to maintain tab
" jumping memory. See: http://superuser.com/q/734074
nnoremap d <M-w>

source! /Users/samuelschmidt/.vimperatorrc.local

" vim: set ft=vimperator: